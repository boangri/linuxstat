  DEF:dw=/var/lib/rrd/linuxstat/rs.xland.ru/prod-app-01/drbd.rrd:dw:AVERAGE
  DEF:dr=/var/lib/rrd/linuxstat/rs.xland.ru/prod-app-01/drbd.rrd:dr:AVERAGE
