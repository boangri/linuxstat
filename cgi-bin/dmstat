 <CENTER>
 <RRD::GRAPH ../html/dmstat/img/cpu-<RRD::CV scale>.gif --lazy 
  --start -<RRD::CV start> -l 0  
  --imginfo '<IMG SRC=/dmstat/img/%s WIDTH=%lu HEIGHT=%lu >'
  --width 600 --title "CPU modes" 
  --vertical-label "percents" 
  DEF:us=/var/lib/rrd/linuxstat/rs.xland.ru/prod-app-01/cpu2.rrd:user:AVERAGE
  DEF:ni=/var/lib/rrd/linuxstat/rs.xland.ru/prod-app-01/cpu2.rrd:nice:AVERAGE
  DEF:sy=/var/lib/rrd/linuxstat/rs.xland.ru/prod-app-01/cpu2.rrd:system:AVERAGE
  DEF:id=/var/lib/rrd/linuxstat/rs.xland.ru/prod-app-01/cpu2.rrd:idle:AVERAGE
  DEF:m1=/var/lib/rrd/linuxstat/rs.xland.ru/prod-app-01/cpu2.rrd:mode1:AVERAGE
  DEF:m2=/var/lib/rrd/linuxstat/rs.xland.ru/prod-app-01/cpu2.rrd:mode2:AVERAGE
  DEF:m3=/var/lib/rrd/linuxstat/rs.xland.ru/prod-app-01/cpu2.rrd:mode3:AVERAGE
  DEF:ncpu=/var/lib/rrd/linuxstat/rs.xland.ru/prod-app-01/misc.rrd:ncpu:AVERAGE
  CDEF:user=us,0,100,ncpu,*,LIMIT 
  CDEF:nice=ni,0,100,ncpu,*,LIMIT 
  CDEF:system=sy,0,100,ncpu,*,LIMIT 
  CDEF:idle=id,0,100,ncpu,*,LIMIT 
  CDEF:mode1=m1,0,100,ncpu,*,LIMIT 
  CDEF:mode2=m2,0,100,ncpu,*,LIMIT 
  CDEF:mode3=m3,0,100,ncpu,*,LIMIT 
  AREA:user#00FF00:"User mode" 
  STACK:nice#FFFF00:"Nice mode" 
  STACK:system#FF0000:"System mode" 
  STACK:mode1#008000:"Mode 1" 
  STACK:mode2#8080FF:"Mode 2" 
  STACK:mode3#C000C0:"Mode 3" 
  STACK:idle#d0d0ff:"**IDLE**"> 
 <BR> 

 <RRD::GRAPH ../html/dmstat/img/runq-<RRD::CV scale>.gif --lazy 
  --start -<RRD::CV start> -l 0  
  --imginfo '<IMG SRC=/dmstat/img/%s WIDTH=%lu HEIGHT=%lu >'
  --width 600 --title "RUNQ" 
  --vertical-label "runq" 
  DEF:ncpu=/var/lib/rrd/linuxstat/rs.xland.ru/prod-app-01/misc.rrd:ncpu:AVERAGE
  DEF:runq=/var/lib/rrd/linuxstat/rs.xland.ru/prod-app-01/misc.rrd:runq:AVERAGE
  CDEF:rq=runq,0,1000,LIMIT 
  AREA:rq#FF0000:"RUNQ" 
  LINE2:ncpu#0000FF:"CPUs"> 
 <BR> 
