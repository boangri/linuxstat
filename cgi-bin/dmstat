 <RRD::GRAPH ../html/dmstat/img/mem-<RRD::CV scale>.gif --lazy 
  --start -<RRD::CV start> -l 0  
  --imginfo '<IMG SRC=/dmstat/img/%s WIDTH=%lu HEIGHT=%lu >'
  --width 600 --title "Memory usage" 
  --vertical-label "Mb" 
  DEF:ram=/var/lib/rrd/linuxstat/rs.xland.ru/prod-app-01/misc.rrd:ram:AVERAGE
  DEF:ramused=/var/lib/rrd/linuxstat/rs.xland.ru/prod-app-01/misc.rrd:ramused:AVERAGE
  DEF:ramfree=/var/lib/rrd/linuxstat/rs.xland.ru/prod-app-01/misc.rrd:ramfree:AVERAGE
  DEF:bucaused=/var/lib/rrd/linuxstat/rs.xland.ru/prod-app-01/misc.rrd:bucaused:AVERAGE
  CDEF:tot=ram,1024,/ 
  CDEF:free=ramfree,1024,/
  CDEF:used=ramused,1024,/
  CDEF:bused=bucaused,1024,/ 
  CDEF:diff=ramused,bucaused,-,1024,/ 
  AREA:bused#FF0000:"Used buffers" 
  STACK:diff#FFc0c0:"Used other"
  STACK:free#d0d0ff:"Free"
  LINE1:used#FF0000 
  LINE1:tot#404080:"Total"> 
 <BR> 

 <RRD::GRAPH ../html/dmstat/img/swap2-<RRD::CV scale>.gif --lazy 
  --start -<RRD::CV start> -l 0 
  --imginfo '<IMG SRC=/dmstat/img/%s WIDTH=%lu HEIGHT=%lu >'
  --width 600 --title "Swap usage" 
