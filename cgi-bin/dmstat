  DEF:swap=/var/lib/rrd/linuxstat/rs.xland.ru/prod-app-01/misc.rrd:swap:AVERAGE
  DEF:swapused=/var/lib/rrd/linuxstat/rs.xland.ru/prod-app-01/misc.rrd:swapused:AVERAGE
  DEF:swapfree=/var/lib/rrd/linuxstat/rs.xland.ru/prod-app-01/misc.rrd:swapfree:AVERAGE
  CDEF:used=swapused,1024,/ 
  CDEF:free=swapfree,1024,/
  CDEF:tot=swap,1024,/
  AREA:used#FF0000:"Used" 
  STACK:free#d0d0ff:"Free"
  LINE1:tot#404080:"Total">
 <BR> 
 
